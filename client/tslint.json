{
  "extends": "@magicspace/configs/tslint-prettier",
  "rules": {
    "no-implicit-dependencies": [
      true,
      [
        "shared",
        "history",
        "reflect-metadata",
        "react-router",
        "mobx-react",
        "axios",
        "utils",
        "injection",
        "stores",
        "services",
        "components",
        "theme",
        "languages",
        "assets"
      ]
    ],
    "import-path-base-url": [true, {"baseUrl": "src"}],
    "no-inferred-empty-object-type": false
  }
}
