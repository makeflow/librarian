{
  "extends": "vts/tslint-prettier",
  "rules": {
    "no-implicit-dependencies": [
      true,
      [
        "history",
        "reflect-metadata",
        "react-router",
        "mobx-react",
        "axios",
        "utils",
        "injection",
        "stores",
        "services",
        "components",
        "theme",
        "languages",
        "assets"
      ]
    ],
    "no-inferred-empty-object-type": false
  }
}
